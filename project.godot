; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=3

[application]

config/name="ld41"
run/main_scene="res://Main.tscn"
config/icon="res://icon.png"

[display]

window/size/width=240
window/size/height=136
window/size/test_width=720
window/size/test_height=408
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[gui]

theme/custom_font="res://DefaultFont.tres"

[layer_names]

2d_physics/layer_1="stage"
2d_physics/layer_2="player"
2d_physics/layer_3="puck"
2d_physics/layer_4="held_puck"
2d_physics/layer_5="target"
2d_physics/layer_6="target_stunned"

[physics]

2d/default_gravity=0

[rendering]

quality/2d/use_pixel_snap=true
environment/default_environment="res://default_env.tres"
